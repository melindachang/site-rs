{
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-astro"],
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 80,
  "semi": false,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "bracketSpacing": true,
  "objectWrap": "collapse",
  "bracketSameLine": true,
  "arrowParens": "avoid",
  "proseWrap": "preserve",
  "endOfLine": "lf"
}
